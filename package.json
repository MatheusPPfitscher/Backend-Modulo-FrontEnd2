{
  "name": "avaliacaofinalbackend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test jest --noStackTrace --runInBand -c ./jest.config.unit.ts",
    "test:integration": "cross-env NODE_ENV=test jest --noStackTrace --runInBand -c ./jest.config.integration.ts",
    "test:all": "cross-env NODE_ENV=test jest --silent --noStackTrace --runInBand",
    "test:setup": "rimraf ./dbtest.sqlite && rimraf ./tests/core/infra/database/migrations && cross-env NODE_ENV=test npm run typeorm migration:generate -- -n TestsMigration && cross-env NODE_ENV=test npm run typeorm migration:run",
    "test:coverage": "cross-env NODE_ENV=test jest --silent --noStackTrace --runInBand --coverage",
    "test:coverage:integration": "cross-env NODE_ENV=test jest --silent --noStackTrace --runInBand --coverage -c ./jest.config.integration.ts",
    "test:watch": "cross-env NODE_ENV=test jest --silent --noStackTrace --runInBand --watchAll",
    "test:verbose": "cross-env NODE_ENV=test jest --verbose --runInBand",
    "dev": "ts-node-dev --respawn --transpile-only ./src/main.ts",
    "typeorm": "ts-node --transpile-only ./node_modules/typeorm/cli.js",
    "postinstall": "npx tsc && npm run typeorm migration:run",
    "start": "node ./dist/main.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "ioredis": "^4.28.5",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.41",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/ioredis": "^4.28.7",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^8.5.7",
    "@types/node": "^17.0.8",
    "@types/redis": "^4.0.11",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "cross-env": "^7.0.3",
    "jest": "^27.5.1",
    "rimraf": "^3.0.2",
    "sqlite3": "^5.0.2",
    "ts-jest": "^27.1.3",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4"
  }
}